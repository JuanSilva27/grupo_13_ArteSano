<html lang="es">
    <%- include("../partials/head.ejs")  %> 
<body>
    <%- include("../partials/header.ejs") %> 

   

 <main class="login">
    <P class="cont.titulo"><h1 class="titulo">INGRESA</h1></P>
    <div class="background">
    <form class="formLogin" action="/users/register" >
          <p>
            <label for="email">CORREO ELECTRONICO</label>
            <input class="tamaño" type="email" placeholder="ej: Ricardo@gmail.com" name="email" >
          </p>
          
          <p>
            <label for="password">CONTRASEÑA:</label>
            <input  class="tamaño" type="password" placeholder="**********" name="password">
          </p>
          <p class="botonLoginContenedor">
              <input  class="botonLogin" type="submit" value="INICIAR SESION">
          </p>
    </form>
  </div>
 </main>
 <%- include("../partials/footer.ejs") %> 
</body>
</html>
